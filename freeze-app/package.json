{
  "dependencies": {
    "@vaadin/router": "^1.7.4",
    "lit": "^2.0.0-rc.2"
  },
  "scripts": {
    "build": "rimraf out && tsc && rollup -c rollup.config.js",
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
    "lint": "lit-analyzer && eslint 'src/**/*.ts'",
    "format": "prettier \"**/*.{js,mjs,ts}\" --ignore-path .gitignore --write",
    "deploy-to-gh-pages": "cd out/dist && echo ilmsfreeze.afq984.org > CNAME && git init -b gh-pages && git add . && git commit -m \"npm run deploy-to-gh-pages\" && git push --force git@github.com:afq984/ilmsfreeze gh-pages"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/wicg-file-system-access": "^2020.9.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@web/dev-server": "^0.1.18",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.8.0",
    "concurrently": "^6.2.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.30.0",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.7",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
