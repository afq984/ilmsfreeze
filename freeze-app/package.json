{
  "dependencies": {
    "@vaadin/router": "^1.7.4",
    "idb": "^6.1.2",
    "lit": "^2.0.0-rc.2",
    "workbox-core": "^6.1.5",
    "workbox-range-requests": "^6.1.5",
    "workbox-routing": "^6.1.5"
  },
  "scripts": {
    "build": "rimraf out && npm run build:tsc && rollup -c rollup.config.js && ln -sf index.html out/dist/404.html",
    "build:tsc": "tsc --build src service-worker",
    "start": "npm run build:tsc && rollup -c rollup-start.config.js && concurrently -k -r \"tsc --build src service-worker --watch --preserveWatchOutput\" \"rollup --watch -c rollup-start.config.js\" \"wds\"",
    "lint": "lit-analyzer && eslint 'src/**/*.ts' 'service-worker/**/*.ts'",
    "format": "prettier \"**/*.{js,mjs,ts}\" --ignore-path .gitignore --write",
    "format:check": "prettier \"**/*.{js,mjs,ts}\" --ignore-path .gitignore --check",
    "deploy-to-gh-pages": "cd out/dist && echo ilmsfreeze.afq984.org > CNAME && git init -b gh-pages && git add . && git commit -m \"npm run deploy-to-gh-pages\" && git push --force git@github.com:afq984/ilmsfreeze gh-pages",
    "test": "npm run build:tsc && web-test-runner out/tsc/**/*.test.js"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.33",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/wicg-file-system-access": "^2020.9.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@web/dev-server": "^0.1.18",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.9.0",
    "@web/test-runner": "^0.13.15",
    "@web/test-runner-puppeteer": "^0.10.0",
    "concurrently": "^6.2.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.31.0",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.3",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
